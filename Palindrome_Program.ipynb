        "def my_palindrome(name):\n",
        "  print(name)\n",
        "  palindrome= name\n",
        "  left= palindrome [0]\n",
        "  right= palindrome [len(palindrome)-1]\n",
        "  while left<=right:\n",
        "    if left == right:\n",
        "      left==left+1\n",
        "      right==right-1\n",
        "      #return false\n",
        "      print(\"true\")"
      ]
